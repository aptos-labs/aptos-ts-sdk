{
  "name": "@aptos-labs/ts-core",
  "version": "5.1.1",
  "description": "Core primitives like addresses, authentication keys, crypto helpers, and error types for the Aptos TypeScript SDK.",
  "license": "Apache-2.0",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/common/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/common/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@aptos-labs/ts-types": "workspace:*",
    "@noble/curves": "^1.9.0",
    "@noble/hashes": "^1.5.0",
    "@scure/bip32": "^1.4.0",
    "@scure/bip39": "^1.3.0",
    "js-base64": "^3.7.7",
    "jwt-decode": "^4.0.0",
    "poseidon-lite": "^0.2.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
